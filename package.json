{
  "name": "eslint-config-typescript-library",
  "description": "ESLint config for typescript library.",
  "version": "0.2.4",
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "keywords": [
    "workflow",
    "eslint config",
    "eslint",
    "typescript",
    "library",
    "typescript library"
  ],
  "scripts": {
    "dev": "tsc -w",
    "build": "tsc",
    "bootstrap": "pnpm install --prefer-offline",
    "lint": "eslint -c .eslintrc.js src/*.ts --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "prepublishOnly": "npm run build",
    "release": "quick-publish"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-financial": "0.1.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.8.10",
    "eslint": "^8.53.0",
    "quick-publish": "^0.7.1",
    "typescript": "5.2.2"
  },
  "files": [
    "index.js",
    "js.js",
    "ts.js",
    "react.js"
  ]
}
